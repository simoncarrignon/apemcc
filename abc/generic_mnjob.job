#!/bin/bash
#SBATCH --job-name=amphora_%j
#SBATCH --output=amphora_%j.out
#SBATCH --error=amphora_%j.err
#SBATCH --ntasks=768
#SBATCH --cpus-per-task=1
#SBATCH --time=02:00:00
#SBATCH --qos=debug



start=`date +%s` #record the time to log the total execution time

mpirun -np 768 python abcpmc-conf.py $1 1

end=`date +%s`

runtime=$((end-start))

echo "total time: $runtime"
